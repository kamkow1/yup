import "$std/io.bc";

main() i32 {

	// classic for loop
	// - skip 4 and 5
	for var i = 0; i < 10; i = i + 1; {

		if i == 4 or i == 5 {
			continue;
		}
	
		fmt_print(" %i ", i);
	}

	// condition-based loop
	// - skip 3 6
	var i = 0;
	for i < 10 {

		if i == 3 or i == 6 {
			i = i + 1;
			continue;
		}
	
		fmt_print(" %i ", i);
		i = i + 1;
	}

	return 0;
}
