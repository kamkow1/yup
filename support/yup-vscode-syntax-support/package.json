{
  "name": "yup-vscode-syntax-support",
  "displayName": "Yup VSCode Syntax Support",
  "description": "vscode support for the yup programming language",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "yup",
      "aliases": ["yup", "yup"],
      "extensions": [".yup"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "yup",
      "scopeName": "source.yup",
      "path": "./syntaxes/yup.tmLanguage.json"
    }]
  }
}
