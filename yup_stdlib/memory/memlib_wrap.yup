import "@std/memory/memlib.ll";

public _malloc(size: i64) -> i8* {
    malloc(size);
}

public _free(ptr: i8*) -> void {
    free(ptr);
}

public _memset(ptr: i8*, value: i32, sz: i64) -> i8* {
    return memset(ptr, value, sz);
}

public _memcpy(dest: i8*, src: i8*, cnt: i64) -> void {
    memcpy(dest, src, cnt);
}
