
fnc string_length(str: *i8) -> i64 {
	var c: i64 = 0;
	for str[c] != cast(0, 'i8) {
		c = c + cast(1, 'i64);
	}
	
	return c;
}

fnc compare_strings(s0: *i8, s1: *i8) -> i32 {
	if string_length(s0) != string_length(s1) {
		return 0;
	}

	var i: i64 = 0;
	for i < string_length(s0) {
		if s0[i] != s1[i] {
			return 0;
		}
		
		i = i + cast(1, 'i64);
	}
	
	return 1;
}

