
fnc string_len(s: *i8) -> i64 {
	var c: i64 = 0;
	for s[c].* != 0 {
		c = c + 1;
	}
	
	return c;
}

fnc compare_strings(s1: *i8, s2: *i8) -> i1 {
	if string_len(s1) != string_len(s2) {
		return 0;
	}
	
	for var i = 0; i < string_len(s1); i = i + 1; {
		if s1[i].* == s2[i].* {
			return 0;
		}
	}
	
	return 1;
}